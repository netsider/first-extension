{
  "manifest_version": 2,

  "name": "Russell's First Google Chrome Extension",
  "description": "Russell's First Google Chrome Extension",
  "version": "1.0",
  
  
  "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["popup.js"]
   }
  ],
  
  "permissions": [
  "http://*/*",
  "https://*/*",
  "contextMenus",
  "activeTab",
  "tabs"
  ],
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}